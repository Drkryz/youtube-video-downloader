    "electron-pack": "electron-packager . --no-prune --ignore=/node_modules --ignore=/e2e --overwrite --ignore=/src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drpidman/youtube-video-downloader.git"
  },
  "keywords": [],
  "author": "DRK",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/drpidman/youtube-video-downloader/issues"
  },
  "homepage": "./",
  "devDependencies": {
    "concurrently": "^6.2.1",
    "cross-env": "^7.0.3",
    "electron": "^13.2.1",
    "electron-builder": "^22.9.1",
    "electron-packager": "^15.3.0",
    "wait-on": "^6.0.0"
  },
  "dependencies": {
    "@craco/craco": "^5.9.0",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "electron-is-dev": "^2.0.0",
    "ffmpeg": "^0.0.4",
    "fluent-ffmpeg": "^2.1.2",
    "module-alias": "^2.2.2",
    "quick.db": "^7.1.3",
