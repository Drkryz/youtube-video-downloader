    "react-dom": "^17.0.2",
    "react-scripts": "^1.1.5",
    "sequelize": "^6.6.5",
    "sqlite3": "^4.2.0",
    "styled-components": "^5.3.0",
    "ytdl": "^1.4.1",
    "ytdl-core": "^4.9.1"
